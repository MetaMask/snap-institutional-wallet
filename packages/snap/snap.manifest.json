{
  "version": "0.0.1",
  "description": "Custodial accounts in MetaMask.",
  "proposedName": "MetaMask Custodial Wallet",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MetaMask/snap-custodial-wallet.git"
  },
  "source": {
    "shasum": "QMUW0DQejZzKHj2+mF5NHmAOJ+AgLqzK7tiuhJ0P4sI=",
    "location": {
      "npm": {
        "filePath": "dist/bundle.js",
        "iconPath": "images/icon.svg",
        "packageName": "@metamask/snap-custodial-wallet-snap",
        "registry": "https://registry.npmjs.org/"
      }
    }
  },
  "initialPermissions": {
    "endowment:network-access": {},
    "endowment:ethereum-provider": {},
    "endowment:keyring": {
      "allowedOrigins": [
        "https://metamask.github.io",
        "metamask.github.io",
        "localhost:8000",
        "http://localhost:8000",
        "https://neptune-custody-ui.dev.metamask-institutional.io"
      ]
    },
    "endowment:rpc": {
      "dapps": true
    },
    "endowment:cronjob": {
      "jobs": [
        {
          "expression": "* * * * *",
          "request": {
            "method": "execute"
          }
        }
      ]
    },
    "snap_manageAccounts": {},
    "snap_manageState": {},
    "snap_dialog": {},
    "endowment:page-home": {}
  },
  "manifestVersion": "0.1"
}
